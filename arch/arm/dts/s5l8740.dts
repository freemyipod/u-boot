/dts-v1/;
#include "skeleton.dtsi"

/ {
    model = "Apple N31";
    compatible = "samsung,s5l8740", "samsung,s5l87xx";

    cpus {
        #address-cells = <1>;
		#size-cells = <0>;
        cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a5";
            reg = <0>;
		};
    };

    memory {
        device_type = "memory";
        reg = <0x08000000 0x04000000>;
    };

    chosen {
        stdout-path = &uart3;
    };

    soc {
        compatible = "simple-bus";
        #address-cells = <1>;
		#size-cells = <1>;

        clkref: clock-ref {
            compatible = "fixed-clock";
            #clock-cells = <0>;
            clock-frequency = <24000000>;
            clock-output-names = "clkref";
        };

        uart3: uart3@3dd00000 {
            compatible = "apple,s5l-uart";
            reg = <0x3dd00000 0x3c>;
            id = <0>;
            clocks = <&clkref>, <&clkref>;
            clock-names = "uart", "clk_uart_baud0";
            reg-io-width = <4>;
            status = "okay";
            bootph-all;
        };
    };
};